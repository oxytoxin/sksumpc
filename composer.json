{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "filament/filament": "^4.0",
    "filament/spatie-laravel-media-library-plugin": "^4.0",
    "guzzlehttp/guzzle": "^7.2",
    "kalnoy/nestedset": "^6.0",
    "laravel/breeze": "^2.0",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.8",
    "lorisleiva/laravel-actions": "^2.7",
    "malzariey/filament-daterangepicker-filter": "^4.0",
    "moneyphp/money": "^4.2",
    "opcodesio/log-viewer": "^3.14",
    "phpoffice/phpspreadsheet": "^1.29",
    "spatie/laravel-data": "^4.0",
    "spatie/laravel-db-snapshots": "^2.5",
    "spatie/laravel-login-link": "^1.3",
    "spatie/laravel-permission": "^6.0",
    "spatie/laravel-ray": "^1.39",
    "spatie/simple-excel": "^3.2",
    "staudenmeir/eloquent-has-many-deep": "^1.7",
    "staudenmeir/laravel-adjacency-list": "^1.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.9",
    "barryvdh/laravel-ide-helper": "^3.0",
    "fakerphp/faker": "^1.9.1",
    "filament/upgrade": "^4.0",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^8.1",
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-laravel": "^v4.0",
    "pestphp/pest-plugin-livewire": "^4.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "files": [
      "app/Oxytoxin/Helpers/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
